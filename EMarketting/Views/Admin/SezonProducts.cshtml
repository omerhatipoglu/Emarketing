
@{
    ViewBag.Title = "SezonProducts";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<style>
    .one {
        margin-top: 100px;
        margin-bottom: 100px;
        margin-right: 100px;
        padding-top: 10px;
        padding-left: 10px;
        padding-right: 10px;
        padding-bottom: 10px;
    }
</style>
<div class="container" ng-init="GetAllSezonProducts()">
    <div class="one">
        <div>


        </div>
        <h1 class="page-header">Sezon Sonu Ürünler </h1>
    </div>
    <div class="one">
        <input type="text" placeholder="Arama" ng-model="ara" />
        <table class="table table-bordered table-hover table-responsive table-striped ">
            <thead>
                <tr>
                    @*<th>Sezon Sonu Ürün ID</th>*@
                    <th>Sezon Sonu Ürün Ad</th>
                    <th>Ürün Resmi</th>

                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="s in sezon |filter:ara">
                    @*<td>{{s.SezonSonuID}}</td>*@
                    <td>{{s.AltUrunlerAd}}</td>
                    <td><img src="{{s.Resim}}"></td>
                </tr>
            </tbody>
        </table>

    </div>


</div>

